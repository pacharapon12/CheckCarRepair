<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#171C22"
    tools:context="com.ziloliz.checkcarrepair.TalkY">



    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="416dp"
        android:layout_height="193dp"
        app:srcCompat="@drawable/d2"
        tools:layout_editor_absoluteX="-2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="526dp" />

    <ImageView
        android:id="@+id/buttonback"
        android:layout_width="50dp"
        android:layout_height="52dp"
        app:srcCompat="@drawable/a6"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.0"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="418dp"
        android:layout_height="67dp"
        app:srcCompat="@drawable/d1"
        tools:layout_editor_absoluteX="-3dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="600dp" />

    <EditText
        android:id="@+id/editText1"
        android:layout_width="311dp"
        android:layout_height="39dp"
        android:layout_marginTop="620dp"
        android:width="100dp"
        android:textSize="24sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginEnd="5dp"
        android:layout_marginLeft="2dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.357"
        />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="57dp"
            android:layout_height="41dp"
            android:layout_marginBottom="4dp"
            android:layout_marginStart="355dp"
            android:layout_marginTop="615dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:srcCompat="@drawable/d3"
            tools:layout_editor_absoluteX="354dp" />

    </LinearLayout>

</android.support.constraint.ConstraintLayout>
