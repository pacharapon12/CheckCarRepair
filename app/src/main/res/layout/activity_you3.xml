<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#171C22"
    tools:context="com.ziloliz.checkcarrepair.You3">

    <ImageView
        android:id="@+id/imageView34"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_marginBottom="580dp"
        android:layout_marginLeft="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@drawable/f1" />

    <ImageView
        android:id="@+id/buttonback"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/a6"
        android:paddingLeft="10dp"
        android:paddingTop="15dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:id="@+id/linearLayout"
        android:layout_marginTop="28dp"
        android:padding="10dp"
        android:weightSum="1"
        android:layout_below="@+id/buttonback"
        android:layout_alignParentStart="true"
        android:layout_margin="50dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="160dp"
            android:orientation="horizontal"
            android:layout_marginBottom="0dp">

            <ImageView

                android:id="@+id/imageView7"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:layout_margin="0dp"
                android:paddingLeft="10dp"
                android:paddingTop="15dp"
                app:srcCompat="@drawable/f2" />


        </LinearLayout>







        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:orientation="horizontal"
            android:layout_marginBottom="0dp">

            <ImageView

                android:id="@+id/imageView5"
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_margin="10dp"
                android:paddingLeft="10dp"
                android:paddingTop="0dp"
                app:srcCompat="@drawable/f4" />


        </LinearLayout>






        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/linearLayout_name"
            android:layout_marginBottom="10dp">

            <ImageView

                android:layout_width="match_parent"
                android:layout_height="110dp"
                android:layout_margin="0dp"
                android:paddingLeft="10dp"
                android:paddingTop="10dp"
                app:srcCompat="@drawable/f3" />


        </LinearLayout>



    </LinearLayout>


</RelativeLayout >
